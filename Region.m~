classdef Region
   properties
      functions
      symbolic_variables
   end
   methods
      function r = Region(string_of_vars, set_of_functions)
          r.symbolic_variables = createSymbolicVariables(string_of_vars);
          r.functions = set_of_functions.Functions;
      end
      
      function r = flatten(obj)
          variable_string = createFlattenedVariableString(obj.symbolic_variables, size(obj.functions,2));
          r = varString2Array(variable_string);
          r = createFlattenedFunction(r,obj.functions, );
      end
   end
end
